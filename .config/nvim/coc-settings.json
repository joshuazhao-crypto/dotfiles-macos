{
  "languageserver": {
    "elixirLS": {
      "command": "~/codes/projects-elixir/clone/elixir-ls/rel/language_server.sh",
      "filetypes": ["elixir", "eelixir"],
      "settings": {
        "elixirLS": {
          "dialyzerEnabled": false
        }
      }
    }
  },
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "javascript.jsx": ["javascript"],
    "typescript": ["javascript"],
    "typescriptreact": ["javascript"]
  },
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "~/.vim/plugged/vim-snippets/snippets"
  ],
  "coc.preferences.formatOnSaveFiletypes": [
    "elixir",
    "css",
    "Markdown",
    "javascript",
    "javascriptreact",
    "json",
    "typescript",
    "typescript.tsx",
    "typescriptreact"
  ],
  "emmet.includeLanguages": { "typescriptreact": "javascriptreact" },
  "tsserver.implicitProjectConfig.experimentalDecorators": true,
  "tsserver.enableJavascript": true,
  "typescript.suggest.completeFunctionCalls": false,
  "javascript.suggest.completeFunctionCalls": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "prettier.arrowParens": "always",
  "prettier.eslintIntegration": true
}
